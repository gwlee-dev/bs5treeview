{
    "name": "vanillajs-bs5treeview",
    "version": "1.2.0",
    "description": "Bootstrap 5 simple treeview with Vanilla JS",
    "keywords": [
        "[\"bstreeview\"",
        "\"twitter\"",
        "\"bootstrap\"",
        "\"bootstrap",
        "5\"",
        "\"tree\"",
        "\"treeview\"",
        "\"tree-view\"",
        "\"navigation\"",
        "\"list-item\"",
        "\"javascript\"",
        "\"es6\""
    ],
    "homepage": "https://github.com/gwlee-dev/vanillajs-bs5treeview#readme",
    "bugs": {
        "url": "https://github.com/gwlee-dev/vanillajs-bs5treeview/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gwlee-dev/vanillajs-bs5treeview.git"
    },
    "license": "Apache-2.0",
    "author": {
        "name": "Sami CHNITER"
    },
    "main": "[\"./dist/css/bstreeview.min.css\",\"./dist/js/bstreeview.min.js\"]",
    "scripts": {
        "js": "terser src/js/bstreeview.js --source-map -o dist/js/bstreeview.min.js",
        "css:sass": "sass src/scss/bstreeview.scss dist/css/bstreeview.css",
        "css:postcss": "postcss dist/css/bstreeview.css --use cssnano -map dist/css/bstreeview.css.map -o dist/css/bstreeview.min.css",
        "build": "npm run js && npm run css:sass && npm run css:postcss"
    }
}
